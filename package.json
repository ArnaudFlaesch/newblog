{
  "name": "portfolio-dev",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "lint:md": "markdownlint **/*.md",
    "lint:md:fix": "npm run lint:md -- --fix",
    "eslint": "eslint src/",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:verify": "npx cypress verify",
    "test": "npm run cy:run",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "copy-locales": "mkdir src/pages/en && cp src/pages/*.astro src/pages/en"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/rss": "^4.0.8",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@fancyapps/ui": "^5.0.36",
    "@formspree/react": "2.5.1",
    "@iconify-json/mdi": "^1.2.0",
    "@iconify-json/ri": "^1.2.0",
    "@iconify-json/simple-icons": "^1.2.5",
    "@mui/icons-material": "6.1.3",
    "@mui/material": "6.1.3",
    "@playform/compress": "^0.1.1",
    "astro": "4.15.12",
    "astro-icon": "^1.1.1",
    "date-fns": "4.1.0",
    "hotkeypad": "0.1.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "1.79.5"
  }
}
